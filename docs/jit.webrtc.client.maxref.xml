<c74object name='jit.webrtc.client' category='utilities'>

	<digest>WebRTC client for Max/MSP </digest>
	<description>WebRTC client for Max/MSP </description>


	<!--METADATA-->

	<metadatalist>
		<metadata name='author'>Jent Zheng </metadata>
		<metadata name='tag'>utilities</metadata>
	</metadatalist>


	<!--ARGUMENTS-->

	<objarglist>

		<objarg name='host' optional='1' type='symbol'>
			<digest>Socket</digest>
			<description>Socket.IO server URL </description>
		</objarg>

		<objarg name='name' optional='1' type='symbol'>
			<digest>Socket</digest>
			<description>Socket.IO room name </description>
		</objarg>

	</objarglist>


