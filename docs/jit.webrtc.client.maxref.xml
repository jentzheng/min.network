			<digest>Disconnect from the remote sockerIO server </digest>
			<description>Disconnect from the remote sockerIO server </description>
		</method>

		<method name='connect'>
			<digest>connect to a remote socketIO server </digest>
			<description>connect to a remote socketIO server </description>
		</method>

	</methodlist>


	<!--ATTRIBUTES-->

	<attributelist>

		<attribute name='host' get='1' set='1' type='symbol' size='1' >
			<digest>remote socker server </digest>
			<description>remote socker server </description>
		</attribute>

		<attribute name='name' get='1' set='1' type='symbol' size='1' >
			<digest>remote socker server room </digest>
			<description>remote socker server room </description>
		</attribute>

	</attributelist>


